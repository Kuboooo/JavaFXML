            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label fx:id="playerName" prefHeight="17.0" prefWidth="91.0" text="Player:" />
            <Button fx:id="sendMessageChat" mnemonicParsing="false" onAction="#posliMasage" text="Chat" GridPane.columnIndex="5" GridPane.rowIndex="7" />
            <TextArea fx:id="textArea" editable="false" prefHeight="105.0" prefWidth="375.0" GridPane.columnSpan="5" GridPane.rowIndex="5" GridPane.rowSpan="2" />
            <Button fx:id="button1" contentDisplay="CENTER" mnemonicParsing="false" onAction="#button1Pressed" prefHeight="96.0" prefWidth="145.0" textAlignment="CENTER" GridPane.columnIndex="2" GridPane.rowIndex="1" />
            <Button fx:id="button2" contentDisplay="CENTER" mnemonicParsing="false" onAction="#button2Pressed" prefHeight="132.0" prefWidth="169.0" GridPane.columnIndex="3" GridPane.rowIndex="1" />
            <Button fx:id="button3" contentDisplay="CENTER" mnemonicParsing="false" onAction="#button3Pressed" prefHeight="162.0" prefWidth="265.0" GridPane.columnIndex="4" GridPane.rowIndex="1" />
            <Button fx:id="button4" contentDisplay="CENTER" mnemonicParsing="false" onAction="#button4Pressed" prefHeight="127.0" prefWidth="176.0" GridPane.columnIndex="2" GridPane.rowIndex="2" />
            <Button fx:id="button5" contentDisplay="CENTER" mnemonicParsing="false" onAction="#button5Pressed" prefHeight="139.0" prefWidth="173.0" GridPane.columnIndex="3" GridPane.rowIndex="2" />
