            <Button fx:id="button9" contentDisplay="CENTER" minWidth="79.0" mnemonicParsing="false" onAction="#button9Pressed" prefHeight="99.0" prefWidth="156.0" GridPane.columnIndex="4" GridPane.rowIndex="3" />
            <TextField fx:id="textFieldMessage" prefHeight="200.0" prefWidth="200.0" promptText="Chat with your opponent" GridPane.columnSpan="5" GridPane.rowIndex="7" />
            <Button fx:id="findOpponent" mnemonicParsing="false" onAction="#findOpponent" prefHeight="111.0" prefWidth="86.0" text="Start" GridPane.columnIndex="3" GridPane.rowIndex="5" />
         </children>
      </GridPane>
   </children>
